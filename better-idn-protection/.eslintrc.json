{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "globals": {
  "chrome": "readonly",
  "punycode": "readonly"
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": ["warn", { "args": "none" }],
    "no-console": "off"
  }
}

